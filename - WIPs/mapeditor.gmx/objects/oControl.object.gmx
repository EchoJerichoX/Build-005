if color=="G" draw_set_color(c_teal)
if color=="R" draw_set_color(c_green)
draw_rectangle(mx,my,mx+7,my+7,0)
draw_set_color(c_white)
draw_set_alpha(0.7)
if wallU="1" draw_line(mx,my,mx+8,my)
if wallD="1" draw_line(mx,my+7,mx+8,my+7)
if wallL="1" draw_line(mx,my,mx,my+8)
if wallR="1" draw_line(mx+7,my,mx+7,my+8)
if wallU="2" {draw_line(mx,my,mx+2,my);draw_line(mx+6,my,mx+8,my)}
if wallD="2" {draw_line(mx,my+7,mx+2,my+7);draw_line(mx+6,my+7,mx+8,my+7)}
if wallL="2" {draw_line(mx,my,mx,my+2);draw_line(mx,my+6,mx,my+8)}
if wallR="2" {draw_line(mx+7,my,mx+7,my+2);draw_line(mx+7,my+6,mx+7,my+8)}

if special="1" draw_sprite(sMapSP,0,mx,my)
if special="2" draw_sprite(sMapSP,1,mx,my)
if special="3" draw_sprite(sMapSP,2,mx,my)
if special="4" draw_sprite(sMapSP,3,mx,my)
if special="5" draw_sprite(sMapSP,4,mx,my)

</string>
