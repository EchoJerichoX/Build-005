        draw_background_ext(IntroBackground,xx/3-350,yy/3-350,1,1,0,c_white,1);
        draw_sprite_ext(sprGunshipThruster,currentthrustimg,x,y,scale,scale,image_angle,c_white,image_alpha*0.9);
        draw_sprite_ext(sprite_index,0,x,y,scale,scale,image_angle,c_white,image_alpha);
    }
    else // Part 2.
    {
        if (instance_exists(oUnknownPlanetMessage))
        {
            if (oUnknownPlanetMessage.image_alpha = 0)
            {
                offset += 1+offsetbooster;
                offsetbooster += 1;
                if (blackalpha &lt; 1) blackalpha += 0.0125;
                if (blackalpha = 1) moverooms -= 1;
                if (moverooms = 0)
                    { scMusic(0,eId.musicselected,1,0); room_goto(rmIntro2); }
            }
        }
        
        draw_background_ext(IntroBackground,0,0-offset*0.15,1,1,0,c_white,1);
        draw_sprite_ext(sprPlanet,0,320,240-offset*0.1,0.85,0.85,planetrot,c_white,1);
        planetrot += 0.005;
        draw_sprite_ext(sprite_index,0,x,y,scale,scale,image_angle,c_white,image_alpha);
        draw_sprite_ext(sprGunshipThruster,currentthrustimg,x,y,scale,scale,image_angle,c_white,image_alpha*0.9);
    }
    if (blackalpha &gt; 0)
    {
        draw_set_alpha(blackalpha);
        draw_set_color(c_black);
        draw_rectangle(0,0,room_width,room_height,0);
    }
    if (whitealpha2 &gt; 0)
    {
        draw_set_alpha(whitealpha2);
        draw_set_color(c_white);
        draw_rectangle(0,0,room_width,room_height,0);
    }
    if (whitealpha1 &gt; 0) draw_background_stretched_ext(IntroWhiteFlash,0,0,640,480,c_white,whitealpha1);
}
if (room = rmIntro2)
{
    if (thrust) draw_sprite_ext(sprGunshipThruster,currentthrustimg,x,y-15*scale,scale,scale,image_angle,c_white,image_alpha*0.9);
    draw_sprite_ext(sprite_index,image_index,x,y,scale,scale,image_angle,c_white,image_alpha);
}
