            <kind>1</kind>
            <string>if (room = rmIntro1)
{
    if (sprite_index = sprGunshipFront) // Part 1.
    {
        speed -= 2.2;
        scalebooster += 0.003;
        scale += 0.05+scalebooster;
        if (image_alpha &lt; 1) image_alpha += 0.1;
        if (y &lt; 25) // Conclude part 1.
        {
            sprite_index = sprGunshipBack;
            speed = 0.65;
            direction = 0;
            whitealpha1 = 1;
            whitealpha2 = 1;
            y = 600;
            x = -400;
            scalebooster = 0.003;
            image_angle = -30;
        }
    }
    else
    {
        move_towards_point(320,240,(point_distance(x,y,320,240))/60);
        scale = (point_distance(x,y,320,240))/100;
        if (speed &lt; 0.025) and (image_alpha &gt; 0) image_alpha -= 0.0125;
        if (speed &lt; 0.35) and (!instance_exists(oUnknownPlanetMessage)) instance_create(320,440,oUnknownPlanetMessage);
        if (image_alpha &lt;= 0) speed = 0;
        if (whitealpha1 &gt; 0) whitealpha1 -= 0.0125;
        if (whitealpha2 &gt; 0) whitealpha2 -= 0.05;
    }
}
thrustersprite += 1;
if (thrustersprite &gt; 4) thrustersprite = 1;
if (thrustersprite = 1) currentthrustimg = 0;
if (thrustersprite = 3) currentthrustimg = 1;
if (room = rmIntro2)
{
    if (go1) // Fly in.
    {
        if (speed &lt; 2) and (image_alpha &lt; 0.5) speed += 0.05;
        if (image_alpha &lt; 1) image_alpha += 0.01;
        //else thrust = 0;
        if (image_alpha &gt;= 0.5) and (speed &gt; 0) speed -= 0.025;
        if (scale &lt; 1)
        {
            scalebooster -= 0.000005;
            scale += 0.003+scalebooster;
        }
        else scale = 1;
        if (scale &gt; 0.7)
            { go2 = 1; go1 = 0; }
    }
    if (go2) // Land.
    {
        if (scale &lt; 1)
        {
            scalebooster -= 0.000015;
            scale += 0.003+scalebooster;
        }
        else scale = 1;
        if (speed &lt; 1) and (y &lt; 300) speed += 0.01;
        if (y &gt; 300) and (speed &gt; 0) speed -= 0.018;
        if (speed &lt; 0) and (!done)
        {
            done = 1;
            speed = 0;
            sound_play(snShipLandingDecompress);
            alarm[3] = 3;
        }
        if (y &gt; landheight-27) and (image_index = 0) image_index = 1;
        if (y &gt; landheight-18) and (image_index = 1)  image_index = 2;
        if (y &gt; landheight-9) and (image_index = 2)  image_index = 3;
        if (y &gt; landheight) and (image_index = 3)  image_index = 4;
    }
}
